config mentohust 'default'
	option enabled '0'
	option username ''
	option password ''
	option nic 'eth0'
	option ip '0.0.0.0'
	option mask '255.255.0.0'
	option gateway '0.0.0.0'
	option dns '0.0.0.0'
	option pinghost '0.0.0.0'
	option timeout '8'
	option echointerval '30'
	option restartwait '15'
	option startmode '0'
	option dhcpmode '0'
	option daemonmode '0'
	option shownotify '0'
	option version '0.00'
	option datafile '/etc/mentohust/'
	option dhcpscript 'udhcpc -i eth0 -q'
